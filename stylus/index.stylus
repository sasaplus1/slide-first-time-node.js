@charset "utf-8"

@font-face
  font-family yasashisa
  src url(yasashisa_gothic.ttf) format("truetype")

.title
  text-align center

h1
  font-family yasashisa, Inconsolata, sans-serif !important

body
  font-family yasashisa, Inconsolata, sans-serif

pre
  background-color black
  color white
  font-family Inconsolata, monospace !important
  font-size 140%
  line-height 1.2em
  padding 1em
